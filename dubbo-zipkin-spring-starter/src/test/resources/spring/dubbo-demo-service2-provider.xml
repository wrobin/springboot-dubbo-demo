<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
	http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <dubbo:protocol name="dubbo" port="20882"/>
    <dubbo:application name="provider2"/>
    <dubbo:registry address="zookeeper://106.15.201.72:2181"/>

    <bean id="service2" class="com.wrobin.zipkin.dubbo.api.Service2Impl"/>

    <dubbo:service interface="com.wrobin.zipkin.dubbo.api.Service2" ref="service2" version="1.0.0"
                   retries="0"/>
</beans>